{
  "version": 2,
  "builds": [
   {
    "src": "server.js",
    "use": "@vercel/node",
    "config": { 
      "includeFiles": ["dist/**"],
      "maxLambdaSize": "50mb"
    }
   }
  ],
  "routes": [
   {
    "src": "/(.*)",
    "dest": "server.js"
   }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 60,
      "memory": 3008
    }
  }
 }